 <c:forEach items="${menuMaps}" var="list">

            <c:forEach items="${list.value}" var="value">
                <c:if test="${value.id == value.parentid}">
                    <dd> <i class="layui-icon">&#xe6c6;</i>${value.name}<i class="Hui-iconfont menu_dropdown-arrow">&#xe6d5;</i></dd>
                </c:if>
            </c:forEach>
            <dd>
                    <c:forEach items="${list.value}" var="value">
                        <c:if test="${value.id != value.parentid}">
                            <a href="javascript:;" data-url="..${value.url}" data-icon="fa-user" data-title="${value.name}" kit-target data-id='2'><i class="fa fa-user" aria-hidden="true"></i><span>${value.name}</span></a>
                        </c:if>
                    </c:forEach>

            </dd>
        </c:forEach>